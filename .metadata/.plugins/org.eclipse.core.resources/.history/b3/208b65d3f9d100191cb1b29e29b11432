
public class isHappy{
	
	 public boolean isHappy(int n) {
	        
		 
		 
		 int sum =n;
		 int temp = n;
		 while(sum!=1) {
			 temp=sum;
			 while(temp!=0) {
				 int rem = temp%10;
				 temp = temp/10;
				 sum = sum + (rem*rem);
			 }
			 
		 }
		 
		 return true;
	    }
	 public static void main(String[] args) {
		System.out.println(new isHappy().isHappy(19));
	}
}