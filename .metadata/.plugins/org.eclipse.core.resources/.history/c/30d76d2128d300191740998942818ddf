import java.util.HashMap;
import java.util.HashSet;

public class myPow {
    public double myPow(double x, int n) {
        
    	if(x==0) {
    		return 0;
    	}
    	if(n ==0 ) {
    		return 1;
    	}
    	
    	double sign = 1;
    	
    	
    	if(x<0) {
    		if(n%2!=0) {
    			sign = -1;
        	}
    	}
    	x = Math.abs(x);
    	
    	
    	
    	
    	double res = 1;
    	int temp =Math.abs(n);
    	
    	HashMap<Integer, Double> hashMap = new HashMap<Integer, Double>();
    	
    	
    	
    	
    	
    	
    	while(temp!=0) {
    		res *= n>0?x:(1/x);
    		temp--;
    	}
    	return res*sign;
    }
    
    public static void main(String[] args) {
		System.out.print(new myPow().myPow(-2, 3));
	}
}
