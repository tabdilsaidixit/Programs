public class XYZ{
	 public boolean isPrime(int n){
	        for(int i =3 ; i<=Math.sqrt(n); i = i+2){
	            if(n%i==0) {
	            	return false;
	            }
	        }
	        
	        return true;
	    }
	 public static void main(String[] args) {
		System.out.print(new XYZ().isPrime(9));
	}
}