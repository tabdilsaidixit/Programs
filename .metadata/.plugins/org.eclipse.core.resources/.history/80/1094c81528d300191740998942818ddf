
public class myPow {
    public double myPow(double x, int n) {
        
    	if(x==0) {
    		return 0;
    	}
    	if(n ==0 ) {
    		return 1;
    	}
    	if(n%2==0) {
    		x = Math.abs(x);
    	}
    	
    	double res = 1;
    	int temp =Math.abs(n);
    	while(temp!=0) {
    		res *= n>0?x:(1/x);
    		temp--;
    	}
    	return res;
    }
    
    public static void main(String[] args) {
		System.out.print(new myPow().myPow(-2, -3));
	}
}
