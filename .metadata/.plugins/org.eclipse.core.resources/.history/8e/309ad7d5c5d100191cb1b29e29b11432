import java.util.ArrayList;
import java.util.List;

public class Pascal {
    public List<List<Integer>> generate(int numRows) {
        
    	List<List<Integer>> list = new  ArrayList<List<Integer>>();
    	
    	if(numRows<=0) {
    		return list;
    	}
    	
    	
    	for(int i=1; i<=numRows;i++ ) {
    		List<Integer> firstRow = new ArrayList<Integer>(i);
    		for(int l=0;l<i;l++) {
    			firstRow.add(1);
    		}
    		firstRow.set(0, 1);
    		firstRow.set(i-1, 1);
    		for(int k=1; k<i-1; k++) {
//    			System.out.println("Row:" + i + "K: "+ k + "list length" + list.size());
//    			List<Integer> previousrow = list.get(0);
//    			System.out.print(previousrow);
//    			
//    			
//    			int o = list.get(i-1).get(k-1);
//    			System.out.println("O:" + o);
//    			
//    			int oo = list.get(i-1).get(k);
//    			System.out.println("O:" + oo);
//    			
//    			
//    			int val = list.get(i-1).get(k-1)+list.get(i-1).get(k);
    			firstRow.set(k, 9);
    		}
  		list.add(firstRow);
    	}
    	return list;
    }
    
    public static void main(String[] args) {
    	List<List<Integer>> xyz = new Pascal().generate(9);
    	
    	List<Integer> j = xyz.get(3);
    	for(int b: j) {
			//System.out.print("f ");
			System.out.print(b+" ");
		}
    	
//    	for(List<Integer> abc: xyz) {
//    		//System.out.print("c ");
//    		for(int b: abc) {
//    			//System.out.print("f ");
//    			System.out.print(b+" ");
//    		}
//    		System.out.println();
//    	}
	}
    
}
