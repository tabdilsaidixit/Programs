
public class isHappy{
	
	 public boolean isHappy(int n) {
	        
		 int count =0;
		 
		 int sum =n;
		 int temp = n;
		 while(sum!=1) {
			 count++;
			 temp=sum;
			 sum=0;
			 while(temp!=0) {
				 int rem = temp%10;
				 temp = temp/10;
				 sum = sum + (rem*rem);
			 }
			 System.out.println(sum);
			 
			 if(count==10) {
				 break;
			 }
		 }
		 
		 return true;
	    }
	 public static void main(String[] args) {
		System.out.println(new isHappy().isHappy(2));
	}
}